<table id="usersTable">
    <thead>
    <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Role</th>
    </tr>
    </thead>
    <tbody>
        {{#each users}}
            <tr data-href="/" class="itemRow">
                <td>{{username}}</td>
                <td>{{firstName}}</td>
                <td>{{lastName}}</td>
                <td>{{roles}}</td>
                <td><button class="deleteButton">Delete</button></td>
            </tr>
        {{/each}}
    </tbody>
</table>